from bs4 import BeautifulSoup as bs
from urllib2 import urlopen
from selenium import webdriver
import json
url="http://www.math.iitb.ac.in/people/ma2014.htm"
browser=webdriver.Chrome()
browser.get(url)
soup=bs(browser.page_source)
data=[]
cl=browser.find_elements_by_xpath("//p/span/a")
i=0
for c in cl:
	'''i+=1
	if(i%3!=0):
		continue'''
	profile={}
	temp=c.text.strip()
	profile["name"]=temp
	data.append(profile)
with open("ma14.json","w") as f:
	json.dump(data,f)	

